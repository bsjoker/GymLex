<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.heathalphaapp.jdls">

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="ru.bakaystas.dot.ChooseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="ru.bakaystas.dot.NewVisitorActivity"
            android:theme="@style/AddTheme">
        </activity>

        <activity android:name="ru.bakaystas.dot.SectionActivity"/>

        <activity android:name="ru.bakaystas.dot.ActiveActivity"/>
    </application>

</manifest>